{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "color": {
            "background": true,
            "custom": false,
            "customDuotone": false,
            "customGradient": false,
            "defaultGradients": true,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [],
            "link": true,
            "palette": [
                {
                    "slug": "awp-green",
                    "color": "#65d676",
                    "name": "AWP Green"
                },
                {
                    "slug": "awp-blue",
                    "color": "#42d1f5",
                    "name": "AWP Blue"
                },
                {
                    "slug": "awp-red",
                    "color": "#c44910",
                    "name": "AWP Red"
                }
            ],
            "text": true
        },
        "spacing": {
            "blockGap": true,
            "margin": true,
            "padding": true
